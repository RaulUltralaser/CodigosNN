function s = sigmoid(b,x)
   s = 1./(1+exp(-b*x)); 
end

function phi=phi_sigmoid(b,xt)
    phi=zeros(20,20);
   for i = 1:20
        for j = 1:20
            phi(i, j) = sigmoid(b, xt(i));
        end
    end
end